QPushButton#MinimizeButton:pressed{
    border-image: url(":/images/dialogs/images/dark_minimise_small_press.png")
}

QPushButton#CloseButton{
    border-image: url(":/images/dialogs/images/dark_close_small_normal.png")
}

QPushButton#CloseButton:hover{
    border-image: url(":/images/dialogs/images/dark_close_small_hover.png")
}


QPushButton#CloseButton:pressed{
    border-image: url(":/images/dialogs/images/dark_close_small_press.png")
}

QPushButton#NormalButton{
    background-color: transparent;
    border: 1px solid rgba(255, 255, 255, 0.3);
    border-radius: 2px;
    color: white;
    text-align: center;
    font-size: 12px;
}

QPushButton#NormalButton:hover{
    background-color: transparent;
    color: rgba(255, 255, 255, 0.5);
}

QPushButton#NormalButton:pressed{
    background-color: transparent;
    color: #01bdff;
}

QPushButton#ActionButton{
    background-color: transparent;
    border: 1px solid rgba(255, 255, 255, 0.3);
    border-right: none;
    border-left: none;
    border-bottom: none;
    color: white;
    text-align: center;
    font-size: 12px;
}

QPushButton#ActionButton:hover{
    background-color: transparent;
    color: rgba(255, 255, 255, 0.5);
}

QPushButton#ActionButton:pressed{
    background-color: transparent;
    color: #01bdff;
}

QPushButton#ConflictButton{
    background-color: transparent;
    border:none;
    color: white;
    text-align: center;
    font-size: 12px;
}

QPushButton#ConflictButton:checked{
    background-color: #ff9327;
}


QLabel#VLine{
    background-color:rgba(255, 255, 255, 0.3);
}

